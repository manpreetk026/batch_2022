<script type="text/javascript">
 var a=prompt("enter a number")
 var year=a
 document.write("entering the loop")
    while(year<100){ 
        if ( year==50){
            break;}
        document.write( year+ "<br/>")
        year++
        
    
    
    }
</script>

            
